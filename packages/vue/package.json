{
    "name": "@freestylejs/ani-vue",
    "author": "freestyle",
    "version": "1.0.1",
    "description": "Ani for vue.",
    "license": "MIT",
    "type": "module",
    "keywords": [
        "vue",
        "animation"
    ],
    "sideEffects": false,
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        "./*": {
            "import": {
                "types": "./dist/*.d.ts",
                "default": "./dist/*.js"
            },
            "require": {
                "types": "./dist/*.d.cts",
                "default": "./dist/*.cjs"
            }
        },
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "scripts": {
        "build": "tsup",
        "build:fast": "tsup --no-dts",
        "dev": "pnpm build --sourcemap --watch",
        "ts:typecheck": "tsc --noEmit --skipLibCheck"
    },
    "dependencies": {
        "@freestylejs/ani-core": "workspace:^"
    },
    "peerDependencies": {
        "vue": ">=3.0.0"
    },
    "devDependencies": {
        "vue": "^3.5.24"
    }
}
